<Page x:Class="Sudoku_WPF.Pages.SavedPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Sudoku_WPF.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800">



    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="8*"/>
        </Grid.RowDefinitions>

        <TextBlock x:Name="title_Txb" Grid.Row="0" Foreground="{DynamicResource Text}" VerticalAlignment="Center" FontSize="30" HorizontalAlignment="Center"/>



        <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled" Grid.Row="1">
            <WrapPanel x:Name="ItemsPanel"  HorizontalAlignment="Center"/>
        </ScrollViewer>
    </Grid>
</Page>
